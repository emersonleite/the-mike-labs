---
import LocationSection from '@/partials/LocationSection.astro';
import About from '@/partials/About.astro';
import About2 from '@/partials/About2.astro';
import Header from '@/partials/Header.astro';
import Base from '@/templates/Base.astro';
import BenefitsSection from '@/partials/BenefitsSection.astro';
import CTASection from '@/partials/CTASection.astro';
import DepoimentSection from '@/partials/DepoimentSection.astro';
import FaqSection from '@/partials/FaqSection.astro';
import TeamSection from '@/partials/TeamSection.astro';
import FooterSection from '@/partials/FooterSection.astro';
import HeroSection from '@/partials/HeroSection.astro';
import ScheduleCalendarSection from '@/partials/ScheduleCalendarSection.astro';

import { AppConfig } from '@/utils/AppConfig';

const { title } = AppConfig;
const { description } = AppConfig;
---

<Base head={{ title, description }}>
  <Header dark={true} />
  <HeroSection />
  <About dark={false} />
  <About2 dark={true} />
  <TeamSection dark={true} />
  <BenefitsSection />
  <DepoimentSection dark={true} />
  <ScheduleCalendarSection dark />
  <FaqSection />
  <LocationSection dark={true} />
  <CTASection dark />
  <FooterSection dark={true} />
</Base>
